{
    "version": "0.2.0",
    "configurations": [
        {
            "name": "Python Debugger",
            "type": "debugpy",
            "request": "launch",
            "module": "fastapi",
            "cwd": "${workspaceFolder}/app",
            "args": ["dev", "main.py", "--host", "0.0.0.0", "--port", "8000", "--reload"],
            "preLaunchTask": "Launch Postgres",
            "postDebugTask": "Stop Postgres",
        },
        {
            "name": "Launch Chrome",
            "type": "chrome",
            "request": "launch",
            "url": "http://localhost:8080",
            "webRoot": "${workspaceFolder}"
        },
    ]
}